{
	"[html]": {
		"editor.formatOnSave": false
	},
	"editor.cursorStyle": "underline",
	"editor.suggestSelection": "first",
	"editor.snippetSuggestions": "none",
	"editor.trimAutoWhitespace": false,
	"editor.renderWhitespace": "boundary",
	"editor.suggest.snippetsPreventQuickSuggestions": false,
	// FILES
	"files.trimFinalNewlines": true,
	"files.insertFinalNewline": true,
	"files.trimTrailingWhitespace": true,
	// OTHER
	"zenMode.restore": true,
	"zenMode.hideTabs": false,
	"less.lint.boxModel": "warning",
	"scss.lint.boxModel": "warning",
	"window.restoreFullscreen": true,
	"window.openWithoutArgumentsInNewWindow": "off",
	// COLOR HELPER
	"colorHelper.formatsOrder": ["hsb", "rgb", "gray", "hex", "named"],
	// YAML
	"yaml.format.singleQuote": true,
	// WIDTH
	"editor.wordWrapColumn": 400,
	"editor.minimap.maxColumn": 400,
	"yaml.format.printWidth": 400,
	"workbench.colorTheme": "One Dark Pro Monokai Darker",
	"workbench.iconTheme": "material-icon-theme",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"typescript.format.enable": false,
	"git.enableSmartCommit": true,
	"explorer.confirmDragAndDrop": false,
	"editor.showFoldingControls": "always",
	"files.eol": "\n",
	"editor.parameterHints.cycle": true,
	"editor.acceptSuggestionOnCommitCharacter": false,
	"editor.suggest.maxVisibleSuggestions": 15,
	"window.newWindowDimensions": "inherit",
	"css.lint.compatibleVendorPrefixes": "warning",
	"css.lint.ieHack": "warning",
	"bracket-pair-colorizer-2.showBracketsInGutter": true,
	"bracket-pair-colorizer-2.showHorizontalScopeLine": false,
	"bracket-pair-colorizer-2.showVerticalScopeLine": false,
	"editor.hideCursorInOverviewRuler": true,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"editor.formatOnSave": true,
	"bracket-pair-colorizer-2.colors": [
		"rgb(255,255,64)",
		"rgb(127,255,127)",
		"rgb(255,127,255)",
		"rgb(79,236,236)"
	],
	"bracket-pair-colorizer-2.colorMode": "Independent",
	"discord.detailsIdle": "",
	"discord.detailsEditing": "{workspace}/{dirname} ({totallines} lines)",
	"discord.detailsDebugging": "{workspace}/{dirname} ({totallines} lines)",
	"discord.lowerDetailsEditing": "{filename} ({filesize})",
	"discord.lowerDetailsDebugging": "{filename} ({filesize})",
	"discord.largeImage": "{LANG}",
	"discord.largeImageIdle": "Idle",
	"discord.lowerDetailsNotFound": "",
	"discord.workspaceElapsedTime": true,
	"discord.lowerDetailsIdle": "Idle",
	"liveServer.settings.donotShowInfoMsg": true,
	"liveServer.settings.port": 8080,
	"workbench.tree.indent": 18,
	"workbench.startupEditor": "none",
	"workbench.commandPalette.history": 0,
	"workbench.tree.renderIndentGuides": "none",
	"workbench.view.alwaysShowHeaderActions": true,
	"explorer.sortOrder": "filesFirst",
	"search.useIgnoreFiles": false,
	"git.autoStash": true,
	"git.showPushSuccessNotification": true,
	"python.formatting.provider": "yapf",
	"python.formatting.yapfArgs": [
		"--style",
		"{USE_TABS: True, INDENT_BLANK_LINES: True, COLUMN_LIMIT: 120, SPACES_BEFORE_COMMENT: 1, BLANK_LINES_AROUND_TOP_LEVEL_DEFINITION: 1}"
	],
	"html.format.enable": false,
	"git.autofetch": true,
	"git.confirmSync": false,
	"editor.matchBrackets": "never",
	"editor.autoClosingBrackets": "never",
	"javascript.validate.enable": true,
	"editor.autoClosingQuotes": "never",
	"terminal.integrated.fontFamily": "monospace",
	"editor.fontFamily": "'Graph 35+ pix', 'Elected Office', 'Office Code Pro', Consolas, 'Courier New', monospace",
	"editor.fontSize": 15,
	"editor.fontLigatures": true,
	"window.zoomLevel": 1,
	"explorer.confirmDelete": false,
	"prettier.tabWidth": 4,
	"prettier.useTabs": true,
	"prettier.singleQuote": true,
	"prettier.quoteProps": "consistent",
	"prettier.trailingComma": "es5",
	"prettier.arrowParens": "always",
	"prettier.endOfLine": "auto",
	"prettier.printWidth": 60,
	"editor.formatOnSaveTimeout": 5000,
	"editor.maxTokenizationLineLength": 1000000,
	"terminal.integrated.rendererType": "dom",
	"terminal.integrated.scrollback": 2000,
	"editor.autoSurround": "never",
	"latex-workshop.view.pdf.viewer": "tab",
	"html.autoClosingTags": false,
	"C_Cpp.default.includePath": ["/usr/bin/g++"],
	"python.defaultInterpreterPath": "/usr/bin/python3",
	"python.languageServer": "Pylance",
	"python.analysis.typeCheckingMode": "basic",
	"python.analysis.diagnosticSeverityOverrides": {
		"reportGeneralTypeIssues": "warning"
	},
	"editor.tabSize": 2,
	"intelephense.phpdoc.useFullyQualifiedNames": true
}
